http:
  script:
    - match: *.wahlap.com
      name: upload
      type: response # request / response
      require-body: true
      timeout: 20
      argument: ''
      binary-mode: false
      max-size: 1048576 # 1MB
 
script-providers:
  upload:
    url: https://raw.githubusercontent.com/M1k0t0/dx-prober-updater-stash-override/main/upload.js
    interval: 86400